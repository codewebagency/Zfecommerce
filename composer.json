{
  "name": "codewebagency/zfecommerce",
  "description": "Zend Framework 2 Module that provides a layer of features for an Ecommerce Platform with EAV Data Modeling",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "module",
    "zf2",
    "ecommerce",
    "eav"
  ],
  "homepage": "https://github.com/codewebagency/Zfecommerce",
  "authors": [
    {
      "name": "Hossein Bahrami Nekoo",
      "email": "info@codewebagency.com",
      "homepage": "http://www.codewebagency.com/"
    }
  ],
  "require": {
    "php": "~5.6 || ~7.0",
    "zf-commons/zfc-rbac": "~2.4",
  },
  "require-dev": {
    "zendframework/zend-authentication": "~2.2",
    "zendframework/zend-developer-tools": "~1.1",
    "zendframework/zend-log": "~2.2",
    "zendframework/zend-http": "~2.2",
    "zendframework/zend-i18n": "~2.7.3",
    "zendframework/zend-serializer": "~2.2",
    "zendframework/zend-view": "~2.2",
    "phpunit/phpunit": "~4.8.26",
    "squizlabs/php_codesniffer": "2.6.*",
    "satooshi/php-coveralls": "~0.6",
  },
  "suggest": {
    "zendframework/zend-developer-tools": "if you want to show information about the store",
    "doctrine/doctrine-module": "if you want to use Doctrine ORM"
  },
  "autoload": {
    "psr-0": {
      "Zfecommerce\\": "src/"
    }
  }
}